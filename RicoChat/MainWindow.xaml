<Window x:Class="RicoChat.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RicoChat"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="300"
        Closing="Window_Closing">
    <Grid Background="Black">
        <ComboBox Name="InputComboBox" HorizontalAlignment="Left" Margin="170,370,0,0" VerticalAlignment="Top" Width="120"/>
        <ComboBox Name="OutputComboBox" HorizontalAlignment="Left" Margin="170,402,0,0" VerticalAlignment="Top" Width="120"/>
    </Grid>
</Window>
